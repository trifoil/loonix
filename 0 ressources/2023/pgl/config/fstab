/dev/mapper/rl-back     /back                   xfs     defaults,noauto,nosuid,noexec        0 0
/dev/mapper/rl-back_log /back/log               xfs     defaults,noauto,nosuid,noexec        0 0
UUID=7096dbb1-ddaa-48fd-8869-f927d98621df /boot                   xfs     defaults        0 0
/dev/mapper/rl-home     /home                   xfs     defaults,nosuid        0 0
/dev/mapper/rl-share    /share                  xfs     defaults,nosuid,noexec        0 0
/dev/mapper/rl-var_log  /var/log                xfs     defaults        0 0
/dev/mapper/rl-www      /www                    xfs     defaults,nosuid,noexec,gquota,uquota       0 0
